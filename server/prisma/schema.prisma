// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model MlbProduct {
  id       Int    @id @default(autoincrement())
  name     String
  price    Float
  image    String
  link     String
  category String
}

model BProduct {
  id       Int    @id @default(autoincrement())
  name     String
  price    Float
  image    String
  link     String
  category String
}
